class Student:

    # [assignment] Skeleton class. Add your code here
  def _init_(self,name,age,track,scores):
      self.name=name 
      self.age=age
      self.track=track 
      self.scores=scores 
       #pass
Bob= Student(name=input("enter your name"), age=input("enter your age"), track=input("enter your track"), score=20.40)

print("Students details:")
print("His name is",Bob.name)
print("age",Bob.age)
print("track",Bob.track)
print("score",Bob.score)
    
# Expected methods
Bob.change_name("Peter")
Bob.change_age(34)
Bob.add_track("UI/UX")
Bob.get_score()

def get_score(self):
    print("Bob score is", self.score)
    return self.score
